#!/bin/sh
for file in /boot/start.elf \
            /boot/start_cd.elf \
            /boot/start_db.elf \
	    /boot/start_x.elf \
	    /boot/fixup.dat \
	    /boot/fixup_cd.dat \
	    /boot/fixup_db.dat \
	    /boot/fixup_x.dat \
	    /boot/bootcode.bin \
	    /boot/LICENCE.broadcom; do
    rm -f $file
    dpkg-divert --package rpikernelhack --remove --rename $file
    sync
done
